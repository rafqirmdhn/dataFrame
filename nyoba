CREATE TABLE tiket{
	kode_tiket varchar(50) NOT NULL PRIMARY KEY,
	no_penerbangan varchar(50) NOT NULL,
	kelas varchar(100) NOT NULL,
	asal varchar(100) NOT NULL,
	tujuan varchar(100) NOT NULL,
	harga int(100) NOT NULL,
	kode_pesawat varchar(50) NOT NULL,
	FOREIGN KEY(kode_pesawat) REFERENCES pesawat(kode_pesawat)
};

CREATE TABLE pesawat(
	kode_pesawat VARCHAR(50) PRIMARY KEY NOT NULL,
	nama_pesawat VARCHAR(50) NOT NULL,
	jenis_pesawat VARCHAR(50) NOT NULL

);


CREATE TABLE maskapai(
	kode_maskapai VARCHAR(50) PRIMARY KEY NOT NULL,
	nama_maskapai VARCHAR(50) NOT NULL


);
